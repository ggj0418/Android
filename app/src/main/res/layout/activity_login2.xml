<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/login_text1"
        android:layout_width="0sp"
        android:layout_height="0sp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.372"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.0276"
        android:fontFamily="@font/applesdgothicneob"
        android:text="결제가 편한 마켓"
        android:textSize="18dp"
        android:textColor="#232328"
        app:layout_constraintStart_toStartOf="@id/start_guideline2"
        app:layout_constraintTop_toTopOf="@id/top_guideline" />


    <ImageView
        android:id="@+id/login_image1"
        android:layout_width="0sp"
        android:layout_height="0sp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.272"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.0316"
        android:src="@drawable/nobrand_logo"
        app:layout_constraintTop_toTopOf="@id/top_guideline2"
        app:layout_constraintStart_toStartOf="@id/start_guideline2"
        />

    <TextView
        android:id="@+id/login_text3"
        android:layout_width="0sp"
        android:layout_height="0sp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.336"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.025"
        android:text="이메일을 입력하세요"
        android:textSize="15sp"
        android:textColor="#232328"
        android:fontFamily="@font/applesdgothicneom"
        app:layout_constraintTop_toTopOf="@id/top_guideline3"
        app:layout_constraintStart_toStartOf="@id/start_guideline2"
        />

    <!--height wrap 필요, cursor_color 확인 -->
    <EditText
        android:id="@+id/login_edit1"
        android:layout_width="0sp"
        android:layout_height="wrap_content"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.833"
        app:layout_constraintTop_toTopOf="@id/top_guideline4"
        app:layout_constraintStart_toStartOf="@id/start_guideline2"
        android:backgroundTint="#FFDD02"
        android:textColor="#232328"
        android:hint="이메일 입력"
        android:inputType="textEmailAddress"
        android:textSize="15sp"
        android:textCursorDrawable="@drawable/cursor_color"
        />
    <TextView
        android:id="@+id/login_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="11sp"
        android:fontFamily="@font/applesdgothicneom"
        app:layout_constraintStart_toStartOf="@id/start_guideline2"
        app:layout_constraintTop_toTopOf="@id/top_guideline10"
        />




    <Button
        android:id="@+id/login_button1"
        android:layout_width="0sp"
        android:layout_height="0sp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.833"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.0658"
        android:text="시작하기"
        android:fontFamily="@font/applesdgothicneob"
        android:textColor="#7B7B7E"
        android:textSize="18sp"
        android:background="@drawable/button_shape"
        app:layout_constraintTop_toTopOf="@id/top_guideline5"
        app:layout_constraintStart_toStartOf="@id/start_guideline2"/>

        <!--guide라인 값 수정 필요, 작대기 맞는지..?-->
        <TextView
            android:id="@+id/login_find_email"
            android:layout_width="0sp"
            android:layout_height="0sp"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintWidth_percent="0.155"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.021"
            android:textSize="13sp"
            android:fontFamily="@font/applesdgothicneob"
            app:layout_constraintTop_toTopOf="@id/top_guideline6"
            app:layout_constraintStart_toStartOf="@id/start_guideline4"
            android:textColor="#A7A7A9"
            android:text="이메일찾기"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="0sp"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.021"
            android:textSize="13sp"
            android:fontFamily="@font/applesdgothicneob"
            app:layout_constraintStart_toStartOf="@id/start_guideline5"
            app:layout_constraintTop_toTopOf="@id/top_guideline6"
            android:textColor="#A7A7A9"
            android:text="ㅣ"/>

        <TextView
            android:id="@+id/login_find_pw"
            android:layout_width="0sp"
            android:layout_height="0sp"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintWidth_percent="0.189"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.021"
            android:textSize="13sp"
            android:fontFamily="@font/applesdgothicneob"
            app:layout_constraintStart_toStartOf="@id/start_guideline6"
            app:layout_constraintTop_toTopOf="@id/top_guideline6"
            android:textColor="#A7A7A9"
            android:text="비밀번호찾기"/>

    <LinearLayout
        android:layout_width="0sp"
        android:layout_height="0sp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.314"
        android:background="#EAEAEA"
        app:layout_constraintStart_toStartOf="@id/start_guideline2"
        app:layout_constraintTop_toTopOf="@id/top_guideline9"
        />

    <TextView
        android:id="@+id/login_text4"
        android:layout_width="0sp"
        android:layout_height="0sp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.188"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.0212"
        android:fontFamily="@font/applesdgothicneob"
        app:layout_constraintTop_toTopOf="@id/top_guideline7"
        app:layout_constraintStart_toStartOf="@+id/start_guideline7"
        android:textColor="#A7A7A9"
        android:text="간편 로그인"
        />

    <LinearLayout
        android:layout_width="0sp"
        android:layout_height="0sp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.314"
        android:background="#EAEAEA"
        app:layout_constraintStart_toStartOf="@id/start_guideline10"
        app:layout_constraintTop_toTopOf="@id/top_guideline9"
        />

        <ImageView
            android:layout_width="0sp"
            android:layout_height="0sp"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintWidth_percent="0.134"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.0658"
            android:src="@drawable/naver_logo"
            app:layout_constraintStart_toStartOf="@id/start_guideline3"
            app:layout_constraintTop_toTopOf="@id/top_guideline8"
            />

        <ImageView
            android:layout_width="0sp"
            android:layout_height="0sp"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintWidth_percent="0.134"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.0658"
            android:src="@drawable/kakao_logo"
            app:layout_constraintTop_toTopOf="@id/top_guideline8"
            app:layout_constraintStart_toStartOf="@id/start_guideline8"
            />
        <ImageView
            android:layout_width="0sp"
            android:layout_height="0sp"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintWidth_percent="0.134"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.0658"
            android:src="@drawable/google_logo"
            app:layout_constraintTop_toTopOf="@id/top_guideline8"
            app:layout_constraintStart_toStartOf="@id/start_guideline9"
            />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.0778" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start_guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.0833" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start_guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.21" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start_guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.275" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start_guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.493" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start_guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.536" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start_guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.422" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start_guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.43" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start_guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.653" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start_guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.601" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/end_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.792" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.0934" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.127" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.287" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.332" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.436" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.53" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.62" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.666" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.629" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.375" />




</androidx.constraintlayout.widget.ConstraintLayout>